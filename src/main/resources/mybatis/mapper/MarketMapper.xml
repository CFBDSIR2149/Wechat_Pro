<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cwd.Mapper.MarketMapper">
    <insert id="addOneMarket" parameterType="com.cwd.Entity.Market">
        <selectKey keyProperty="id" order="BEFORE"
                   resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert  into market(nickName,avatarUrl,openid,article,price,feature,mark,contact,images)
        values(#{nickName},#{avatarUrl},#{openid},#{article},#{price},#{feature},#{mark},#{contact},#{images})
    </insert>
    <select id="getMarketList" resultType="com.cwd.Entity.Market">
        select * from market
    </select>
</mapper>